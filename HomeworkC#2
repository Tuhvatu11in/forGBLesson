// Задание 1

string input = Console.ReadLine();
Console.WriteLine(input[1];);

// Задание 2

string input = Console.ReadLine();
if (input.Length >= 3)
{
    Console.WriteLine(input[2];);
}
else
{
    Console.WriteLine("Третьей цифры нет");
}

// Задание 3

string input = Console.ReadLine();
switch (input)
{
    case "6":
    case "7":
        Console.WriteLine("Да");
        break;
    default:
        Console.WriteLine("Нет");
        break;
}
